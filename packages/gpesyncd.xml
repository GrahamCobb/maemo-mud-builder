<?xml version="1.0"?>
<package>
  <fetch type="command">
    <command>
    pkg="gpesyncd"
    date="20070421"
    branch="svn://projects.linuxtogo.org/svn/gpe/trunk/base"
    pkgdir="$pkg-0svn${date}maemo"
    svn export -r {${date}T0000Z} $branch/$pkg $pkgdir
    </command>
    <depends>libmimedir-gnome, libgpewidget, libgpepimc, libgpevtype, libsqlite0, libcontactsdb, libeventdb, libtododb</depends>
  </fetch>
  <build>
    <configure-append>--enable-hildon --disable-cairo</configure-append>
  </build>
  <deb>
    <section>user/pim</section>
    <maintainer>Graham Cobb &lt;g+770@cobb.uk.net&gt;</maintainer>
    <description>gpe packaged with mud</description>
  </deb>
</package>
